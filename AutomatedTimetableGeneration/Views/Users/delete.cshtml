@model AutomatedTimetableGeneration.Models.AspNetUser

@{
    ViewBag.Title = "delete";
    Layout = "~/Views/Shared/_LayoutMetronic.cshtml";
}

<span class="label label-danger">Note!</span>

<h4>Are you sure you want to delete this user?</h4>

<div>
  
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Email)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Email)
        </dd>

       

        <dd>
            @Html.HiddenFor(model => model.EmailConfirmed)
        </dd>

        

        <dd>
            @Html.HiddenFor(model => model.PasswordHash)
        </dd>

        

        <dd>
            @Html.HiddenFor(model => model.SecurityStamp)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.PhoneNumber)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.PhoneNumber)
        </dd>

        <dd>
            @Html.HiddenFor(model => model.PhoneNumberConfirmed)
        </dd>

     
        <dd>
            @Html.HiddenFor(model => model.TwoFactorEnabled)
        </dd>

       

        <dd>
            @Html.HiddenFor(model => model.LockoutEndDateUtc)
        </dd>

        <dd>
            @Html.HiddenFor(model => model.LockoutEnabled)
        </dd>

       

        <dd>
            @Html.HiddenFor(model => model.AccessFailedCount)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.UserName)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.UserName)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Experience)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Experience)
        </dd>

        <dt>
            @Html.DisplayName("External")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.isExternal)
        </dd>


        <dd>
            @Html.HiddenFor(model => model.type)
        </dd>

        <dt>
            @Html.DisplayName("Department")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Department.Name)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-circle green" /> |
            @Html.ActionLink("Back to List", "Index")
        </div>
    }
</div>
